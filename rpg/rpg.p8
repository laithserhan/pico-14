pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

---start--------------
-- pico-8
function _init()
 dbg = "" -- debug message
 t = 0 --timer
 actors={} -- 1:player
 cam = {}
 cam.x=0
 cam.y=0
 state=0 -- 0=menu, 1=explore, 2=fight
 mus_pat=0 --  0=menu, 1=explore, 2=fight
 music(mus_pat)
 for x=1,1 do
  actor_new()
 end
 -- music(0)
end

function _draw()
 cls(0)

 if state == 0 then 
  -- menu rendering
   print("pico-8 rpg framework", 25, 16, 7) -- name of the game
   print("press x to start", 32, 64, 7) -- start game instructions
 elseif state == 1 then
  -- explore rendering
  cls(3)
  map(0, 0, 0, 0, 128, 32)
  camera(cam.x, cam.y)
  rectfill(80,80,120,100,12)
  foreach(actors, actor_render)
 elseif state == 2 then
  -- fight rendering
  cls(3)
 end  
  debug(118 - (#dbg*3), 2)
end

function _update60()
 t += 1

 if state == 0 then 
  update_menu()
 elseif state == 1 then
  update_explore()
 elseif state == 2 then
  update_fight()
 end
end
-- pico-8
---end--------------

function debug(x,y)
 print(dbg,cam.x + x,cam.y + y)
end

---start--------------
-- cam
function cam_update()
 cam.x = actors[1].x - 62
 cam.y = actors[1].y - 96
end
-- cam
---end--------------

---start--------------
-- actors
-- updates the actor based on velocity
function actor_update(actor)
 -- movement
 actor.x+=actor.vx
 actor.y+=actor.vy

 if actor.vx < 0 and actor.dirx > 0 
  or actor.vx > 0 and actor.dirx < 0 then 
  actor.dirx = actor.vx
 end
 if actor.vy < 0 and actor.diry > 0 
  or actor.vy > 0 and actor.diry < 0 then 
  actor.diry = actor.vy
 end 

 -- only update every 8th frame
 if t % 8 == 0 then
 -- which sprite to render?
  if abs(actor.vx) < 0.1 and abs(actor.vy) < 0.1 then
   -- idle
   actor.frame = actor.spr_index
  else
   actor.frame = actor.spr_index + ((actor.frame) % actor.frames)
  end  
 end 

 -- ease out
 actor.vx *= 0.7
 actor.vy *= 0.7
end

-- rendered the actor in the draw event
function actor_render(actor)
 spr(actor.frame,actor.x,actor.y)
end

-- creates a new actor and returns its id
function actor_new()
 local actor = {}
 actor.x=0
 actor.y=118
 actor.vx=0
 actor.vy=0
 actor.dirx=0
 actor.diry=0
 actor.health=1
 actor.spd=1
 actor.spr_index=1
 actor.frame=1
 actor.frames=2
 add(actors, actor)
 return #actors
end
-- actors
---end----------------

---start--------------
-- physics

-- test if a point is solid
function solid (x, y)
  if (x < 0 or x >= 128 ) then
    return true end
        
  val = mget(x, y)
  return fget(val, 0)
end
-- physics
---end----------------

---start--------------
-- states
function update_menu()
 if mus_pat != 0 then
  music(0)
  mus_pat = 0
 end

 if btn(5) then state += 1 end
end

function update_explore()
 if mus_pat < 1 or mus_pat > 4 then
  music(1)
  mus_pat = 1
 end

 local vx = actors[1].vx
 local vy = actors[1].vy
 
 if btn(0) then
  vx = -actors[1].spd
 elseif btn(1) then
  vx = actors[1].spd
 end

 if btn(2) then
  vy = -actors[1].spd
 elseif btn(3) then
  vy = actors[1].spd
 end 
 
 local	actor = actors[1]
 actor.vx = vx
 actor.vy = vy

 foreach(actors, actor_update)
 cam_update()
 dbg = "actors "..#actors
end

function update_fight()
 if mus_pat != 5 then
  music(5)
  mus_pat = 5
 end
end
-- states
---end----------------
