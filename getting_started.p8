pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function debug(x,y)
 print(dbg,x,y)
end

---start--------------
-- cam
function cam_update()
 cam.x = actors[1].x - 62
 cam.y = actors[1].y - 96
end

-- cam
---end--------------

---start--------------
-- actors

-- updates the actor based on velocity
function actor_update(actor)
 -- movement
 actor.x+=actor.vx
 actor.y+=actor.vy

 if actor.vx < 0 and actor.dirx > 0 
  or actor.vx > 0 and actor.dirx < 0 then 
  actor.dirx = actor.vx
 end

 -- only update every 8th frame
 if t % 8 == 0 then
 -- which sprite to render?
  if abs(actor.vx) < 0.1 then
   -- idle
   actor.frame = actor.spr_index
  else
    actor.frame = actor.spr_index + ((actor.frame) % actor.frames)
  end  
 end 
end

-- rendered the actor in the draw event
function actor_render(actor)
 spr(actor.frame,actor.x,actor.y)
end

-- creates a new actor and returns its id
function actor_new()
 local actor = {}
 actor.x=0
 actor.y=118
 actor.vx=0
 actor.vy=0
 actor.dirx=0
 actor.spd=1
 actor.spr_index=1
 actor.frame=1
 actor.frames=2
 add(actors, actor)
 return #actors
end
-- actors
---end----------------

---start--------------
-- physics

function _init()
 dbg = "" -- debug message
 t = 0 --timer
 actors={} -- 1:player
 cam = {}
 cam.x=0
 cam.y=0
 for x=1,1 do
  actor_new()
 end
 music(0)
end

function _draw()
 cls(7)
 map(0, 0, 0, 0, 128, 32)

 camera(cam.x, cam.y)
 rectfill(80,80,120,100,12)

 foreach(actors, actor_render)
 debug(118 - (#dbg*3), 2)
end

function _update60()
 t += 1
 dbg = "actors "..#actors
 local vx = actors[1].vx
 
 if btn(0) then
  vx = -actors[1].spd
 elseif btn(1) then
  vx = actors[1].spd
 else
  vx *= 0.7
 end
 
 local	actor = actors[1]
 actor.vx = vx
 actor.vy = 0

 foreach(actors, actor_update)
 cam_update()
end 
